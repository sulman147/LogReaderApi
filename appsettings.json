{
  "ConnectionStrings": {
    "elastic-search": "Nodes=https://localhost:9200;DefaultIndex=windows-logs;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "elasticsearch": {
    "index": "windows-logs",
    "url": "https://localhost:9200/"
  },
  "AllowedHosts": "*",
  "LogReadInterval": 5,
  "EventLogs": [ "Application", "Security", "System" ] // Example log names
}
